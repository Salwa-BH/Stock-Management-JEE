<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CreateMarque</servlet-name>
        <servlet-class>S_Pack.CreateMarque</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateProduit</servlet-name>
        <servlet-class>S_Pack.CreateProduit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>java</servlet-name>
        <servlet-class>CreateProduit.java</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateStocks</servlet-name>
        <servlet-class>S_Pack.CreateStocks</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddProductsToStock</servlet-name>
        <servlet-class>S_Pack.AddProductsToStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveStock</servlet-name>
        <servlet-class>S_Pack.RemoveStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveProduct</servlet-name>
        <servlet-class>S_Pack.RemoveProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveProducts</servlet-name>
        <servlet-class>S_Pack.RemoveProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteProductFromStock</servlet-name>
        <servlet-class>S_Pack.DeleteProductFromStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifierProduit</servlet-name>
        <servlet-class>S_Pack.ModifierProduit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteMarque</servlet-name>
        <servlet-class>S_Pack.DeleteMarque</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProduitsMarque</servlet-name>
        <servlet-class>S_Pack.ProduitsMarque</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateMarque</servlet-name>
        <url-pattern>/CreateMarque</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateProduit</servlet-name>
        <url-pattern>/CreateProduit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>java</servlet-name>
        <url-pattern>/java</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateStocks</servlet-name>
        <url-pattern>/CreateStocks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProductsToStock</servlet-name>
        <url-pattern>/AddProductsToStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveStock</servlet-name>
        <url-pattern>/RemoveStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveProduct</servlet-name>
        <url-pattern>/RemoveProduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveProducts</servlet-name>
        <url-pattern>/RemoveProducts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteProductFromStock</servlet-name>
        <url-pattern>/DeleteProductFromStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifierProduit</servlet-name>
        <url-pattern>/ModifierProduit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteMarque</servlet-name>
        <url-pattern>/DeleteMarque</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProduitsMarque</servlet-name>
        <url-pattern>/ProduitsMarque</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
